@article{keltner2003,
  title = {Power, Approach, and Inhibition},
  author = {Keltner, Dacher and Gruenfeld, Deborah H. and Anderson, Cameron},
  year = {2003},
  month = apr,
  journal = {Psychological Review},
  volume = {110},
  number = {2},
  pages = {265--284},
  issn = {0033-295X},
  doi = {10.1037/0033-295x.110.2.265},
  abstract = {This article examines how power influences behavior. Elevated power is associated with increased rewards and freedom and thereby activates approach-related tendencies. Reduced power is associated with increased threat, punishment, and social constraint and thereby activates inhibition-related tendencies. The authors derive predictions from recent theorizing about approach and inhibition and review relevant evidence. Specifically, power is associated with (a) positive affect, (b) attention to rewards, (c) automatic information processing, and (d) disinhibited behavior. In contrast, reduced power is associated with (a) negative affect; (b) attention to threat, punishment, others' interests, and those features of the self that are relevant to others' goals; (c) controlled information processing; and (d) inhibited social behavior. The potential moderators and consequences of these power-related behavioral patterns are discussed.},
  langid = {english},
  pmid = {12747524}
}

@incollection{keltner2010,
  title = {Paradoxes of Power: Dynamics of the Acquisition, Experience, and Social Regulation of Social Power.},
  booktitle = {The Social Psychology of Power.},
  author = {Keltner, Dacher and Gruenfeld, Deborah and Galinsky, Adam and Kraus, Michael W.},
  year = {2010},
  pages = {177--208},
  publisher = {{Guilford Press}},
  address = {{New York,  NY,  US}},
  abstract = {Although a great deal of research points to corruption and malevolent facets of power, instances of prosocial behavior have also been documented. In Chapter 6, "Paradoxes of Power: Dynamics of the Acquisition, Experience, and Social Regulation of Social Power," Keltner, Gruenfeld, Galinsky, and Kraus deal with these inconsistencies, pointing out paradoxical effects of power. With respect to the acquisition of social power, they show that power is afforded to those individuals who engage socially in ways that build bonds that advance the interests of the group. With respect to the experience of power, they review evidence showing that power prioritizes the emotions, goals, and actions of high-power individuals, often leading to socially disruptive objectification of others. Finally, with respect to constraints upon power, they reveal how group-based representations (a fellow group member's reputation), communication (gossip), and self-assessments (an individual's modest sense of power) constrain the actions of those in power according to how they advance group interests. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  isbn = {978-1-60623-619-2 (Hardcover)}
}

