@article{willemsen2018,
  title = {A New Look at the Attribution of Moral Responsibility: The Underestimated Relevance of Social Roles},
  shorttitle = {A New Look at the Attribution of Moral Responsibility},
  author = {Willemsen, Pascale and Newen, Albert and Kaspar, Kai},
