@article{cyranowski1999,
  title = {The Role of Sexual Self-Schema in a Diathesis\textendash Stress Model of Sexual Dysfunction},
  author = {CYRANOWSKI, JILL M. and AARESTAD, SUSAN L. and ANDERSEN, BARBARA L.},
  year = {1999},
  journal = {Applied \& preventive psychology : journal of the American Association of Applied and Preventive Psychology},
  volume = {8},
  number = {3},
  pages = {217--228},
  issn = {0962-1849},
  doi = {10.1016/S0962-1849(05)80078-2},
  abstract = {Sexual self-schemas are cognitive generalizations regarding sexual aspects of the self that represent a core component of one's sexuality. We contend that individual differences in the sexual self-view represent an important cognitive diathesis for predicting sexual difficulty or dysfunction. We illustrate the role of sexual self-schemas on sexual behavior and responsiveness in healthy female and male samples. Next, we describe how diathesis\textendash stress models of psychopathology have been applied to the sexual arena, and discuss the critical features of clinically useful diathesis variables. Drawing from these criteria, we examine the diathetic properties of sexual self-schemas. Finally, we discuss an empirical test of the proposed diathesis\textendash stress interaction, reviewing the role of women's sexual self-views on sexual morbidity following diagnosis and treatment for gynecologic cancer.},
  pmcid = {PMC2706507},
  pmid = {19587834}
}

@article{dane2007,
  title = {Exploring Intuition and Its Role in Managerial Decision Making},
  author = {Dane, Erik and Pratt, Michael G.},
  year = {2007},
  journal = {The Academy of Management Review},
  volume = {32},
  number = {1},
  pages = {33--54},
  publisher = {{Academy of Management}},
  address = {{US}},
  issn = {1930-3807(Electronic),0363-7425(Print)},
  doi = {10.2307/20159279},
  abstract = {We review and reconceptualize "intuition," defining intuitions as affectively charged judgments that arise through rapid, nonconscious, and holistic associations. In doing so, we delineate intuition from other decision-making approaches (e.g., insight, rational). We also develop a model and propositions that incorporate the role of domain knowledge, implicit and explicit learning, and task characteristics on intuition effectiveness. We close by suggesting directions for future research on intuition and its applications to managerial decision making. (PsycINFO Database Record (c) 2016 APA, all rights reserved)}
}

