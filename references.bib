  author = {Lopez, Vera and {Chesney-Lind}, Meda and Foley, Julia},
  year = {2012},
  month = jun,
  journal = {Violence Against Women},
  volume = {18},
  number = {6},
  pages = {681--690},
  issn = {1077-8012, 1552-8448},
  doi = {10.1177/1077801212454112},
  abstract = {We drew on the theory of gender and power and grounded theory methodology to explore how 18 Latina girls conceptualized power and control within their heterosexual dating relationships. Our findings indicate that boys/men used a number of strategies to control girls, including: regulating appearances and behaviors; cheating and threatening to cheat; and physical and sexual violence. Girls used a variety of strategies to resist these attempts to control them, including: lying, flirting, and cheating; reactive violence; breaking up; and maintaining emotional distance. Girls attempted to subvert boys' attempts to control them; however, these attempts were not always successful given the constraints of gender that adolescent females must negotiate.},
  langid = {english}
}

