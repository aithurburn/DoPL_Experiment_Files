	month = jan,
	year = {2019},
	keywords = {dilemmas, Morality, affect, adults, Arousal, death v nondeath, deontological, Older v younger, viral},
	pages = {47--60},
	file = {Age differences in moral judgment.docx:C\:\\Users\\s1932788\\Zotero\\storage\\GMEBRZWK\\Age differences in moral judgment.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;McNair et al. - 2019 - Age differences in moral judgment Older adults ar.pdf:C\:\\Users\\s1932788\\Zotero\\storage\\8QUZSUVZ\\McNair et al. - 2019 - Age differences in moral judgment Older adults ar.pdf:application/pdf}
